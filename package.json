{
  "name": "my_react_starter_kit",
  "version": "1.0.0",
  "description": "Simple Starter kit [webpack,react,react-router,recompose,lodash,emotion]",
  "main": "index.js",
  "scripts": {
    "lint": "tslint src/**/*.{ts,tsx}",
    "format": "prettier --write --list-different --single-quote --tab-width=4 --print-width=120 webpack.config.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --env.dev --hot --open",
    "build": "webpack  --env.prod",
    "build:dev": "webpack --env.dev",
    "test": "jest --config config/jest/unit.config.js"
  },
  "keywords": [
    "react",
    "react-router",
    "recompose",
    "emotionJS",
    "typescript"
  ],
  "author": "Aigars Beinarovics,Arthur Pumpa",
  "license": "ISC",
  "dependencies": {
    "emotion": "^7.2.0",
    "lodash": "^4.17.4",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "mobx-react-form": "^1.32.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flexbox-grid": "^1.1.5",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.0",
    "rosemary-ui": "^1.0.237",
    "theming": "^1.1.0",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router-dom": "^4.0.7",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^21.0.2",
    "mobx-react-devtools": "^4.2.15",
    "npm": "^5.4.0",
    "prettier": "^1.5.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.7.1"
  }
}
